        },
        {
            "name": "砂糖",
            "quantity": "小さじ1/2"
        },
        {
            "name": "だしの素(顆粒)",
            "quantity": "小さじ1/2"
        },
        {
            "name": "醤油",
            "quantity": "小さじ2～"
        }
